(whitelistIP) {
                remote_ip 0.0.0.0 1.1.1.1
        }

http:// {
        log {
                output stderr
                format filter {
                        wrap console
                        fields {
                                request>headers>Authorization delete
                        }
                }
        }
        @beaconapi {
                import whitelistIP
                path /beaconapi/*
        }
        handle @beaconapi {
                reverse_proxy http://lighthouse-archival:5052
                uri strip_prefix /beaconapi
        }

		@elapi {
                import whitelistIP
                path /elapi/*
        }
        handle @elapi {
                reverse_proxy http://lighthouse-archival:5052
                uri strip_prefix /elapi
        }


}